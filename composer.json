{
    "name": "ezha/purwantaraphp",
    "description": "Package Laravel For Integration To Purwantara√ü",
    "license": "MIT",
    "authors": [
        {
            "name": "Muh Ezha Syafaat",
            "email": "ezha@purwantara.id",
            "homepage": "https://ezxxcode.com"
        }
    ],
    "homepage": "https://github.com/ezhasyafaat/purwantaraphp",
    "keywords": ["Laravel", "PurwantaraPHP","Payment Gateway"],
    "require": {
        "illuminate/support": "~7|~8",
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Ezha\\PurwantaraPHP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ezha\\PurwantaraPHP\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ezha\\PurwantaraPHP\\PurwantaraPHPServiceProvider"
            ],
            "aliases": {
                "PurwantaraPHP": "Ezha\\PurwantaraPHP\\Facades\\PurwantaraPHP"
            }
        }
    }
}
